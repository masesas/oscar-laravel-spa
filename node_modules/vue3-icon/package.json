{
  "name": "vue3-icon",
  "version": "2.1.0",
  "description": "Icon component for Vue 3",
  "main": "./dist/vue3-icon.js",
  "module": "./dist/vue3-icon.mjs",
  "browser": "./dist/vue3-icon.umd.js",
  "unpkg": "./dist/vue3-icon.umd.js",
  "exports": {
    ".": {
      "require": "./dist/vue3-icon.js",
      "import": "./dist/vue3-icon.mjs"
    }
  },
  "files": ["dist","CHANGELOG.md"],
  "scripts": {
    "build": "vite build -c vite-build.config.js",
    "test": "vitest",
    "demo": "vite",
    "demo:build": "vite build",
    "demo:deploy": "gh-pages -d demo/dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/craigrileyuk/vue3-icon.git"
  },
  "keywords": [
    "Vue",
    "icon",
    "mdi",
    "pictogrammers",
    "fontawesome"
  ],
  "author": "Pictogrammers, James Coyle, Craig Riley",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/craigrileyuk/vue3-icon/issues"
  },
  "homepage": "https://github.com/craigrileyuk/vue3-icon#readme",
  "devDependencies": {
    "@fortawesome/free-brands-svg-icons": "^6.2.0",
    "@fortawesome/free-regular-svg-icons": "^6.2.0",
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "@mdi/js": "^7.0.96",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vue/test-utils": "^2.0.0-beta.13",
    "bili": "^5.0.0",
    "browser-env": "^3.2.1",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-vue": "^9.6.0",
    "gh-pages": "^4.0.0",
    "happy-dom": "^7.5.10",
    "prettier": "^2.2.1",
    "prismjs": "^1.22.0",
    "sass": "^1.30.0",
    "simple-icons": "^7.15.0",
    "vite": "^3.1.8",
    "vitest": "^0.24.1",
    "vue": "^3.2.40",
    "vue-jest": "^5.0.0-alpha.7"
  }
}
