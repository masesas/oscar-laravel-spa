{
	"name": "vue3-snackbar",
	"version": "2.2.1",
	"author": "Craig Riley",
	"scripts": {
		"build": "vite build -c vite-build.config.js",
		"build:types": "jsdoc src -c ./jsdoc-conf.json",
		"demo": "vite",
		"demo:build": "vite build",
		"demo:deploy": "gh-pages -d demo/dist",
		"prepublishOnly": "npm run build",
		"preinstall": "npx only-allow pnpm",
        "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
        "format": "prettier .  --write"
	},
	"type": "module",
	"types": "./types.d.ts",
	"main": "./dist/vue3-snackbar.cjs",
	"module": "./dist/vue3-snackbar.js",
	"exports": {
		".": {
			"require": "./dist/vue3-snackbar.cjs",
			"import": "./dist/vue3-snackbar.js",
			"browser": "./dist/vue3-snackbar.umd.cjs"
		},
		"./styles": "./dist/style.css"
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"tiny-emitter": "^2.1.0",
		"vue3-icon": "^2.1.0"
	},
	"devDependencies": {
		"@mdi/js": "^7.4.47",
		"@vitejs/plugin-vue": "^5.0.4",
		"@vueuse/core": "^10.9.0",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-vue": "^9.23.0",
		"gh-pages": "^6.1.1",
		"jsdoc": "^3.6.11",
		"jsdoc-vuejs": "^4.0.0",
		"postcss": "^8.4.38",
		"postcss-nested": "^6.0.1",
		"prettier": "^3.2.5",
		"prismjs": "^1.29.0",
		"roboto-fontface": "^0.10.0",
		"sass": "^1.72.0",
		"sass-loader": "^13.3.3",
		"tsd-jsdoc": "^2.5.0",
		"vite": "^5.2.3",
		"vite-plugin-vuetify": "^2.0.3",
		"vue": "^3.4.21",
		"vuetify": "^3.5.11"
	},
	"peerDependencies": {
		"vue": "^3.0.0"
	},
	"browserslist": [
		"> 1%",
		"last 2 versions",
		"not dead"
	],
	"bugs": {
		"url": "https://github.com/craigrileyuk/vue3-snackbar/issues"
	},
	"homepage": "https://github.com/craigrileyuk/vue3-snackbar#readme",
	"keywords": [
		"Vue",
		"Vue3",
		"snackbar",
		"toast",
		"alerts",
		"messages"
	],
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/craigrileyuk/vue3-snackbar.git"
	},
